{
  "name": "app-router-redux-cypress-daisyui",
  "version": "0.1.0",
  "author": "Rahad Rahman (https://github.com/rahadkc)",
  "private": true,
  "scripts": {
    "git-ready": "git init && npm run setup:husky",
    "dev": "next dev -p 3050",
    "build": "next build",
    "start": "next start -p 3050",
    "lint": "next lint",
    "lint:biome": "npx biome lint .",
    "lint:ci": "npx biome ci --formatter-enabled=true  --linter-enabled=true --organize-imports-enabled=true .",
    "format": "npx @biomejs/biome check --apply .",
    "format:src": "npx @biomejs/biome format ./src --write",
    "test": "jest",
    "test:verbose": "jest --watchAll --verbose",
    "test:exit": "jest --forceExit",
    "test:clear": "jest --clearCache",
    "find:unused": "next-unused",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "check-types": "tsc --noEmit --pretty",
    "cypress:open": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:3050 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3050 \"cypress run --e2e\"",
    "e2e:component": "cypress open --component",
    "e2e:component:headless": "cypress run --component",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "build-storybook": "storybook build",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://127.0.0.1:6006 test-storybook",
    "lint-staged": "lint-staged --config lint-staged.config.js",
    "setup:husky": "npm pkg set scripts.prepare=\"husky install\" && npm run prepare && npx husky add .husky/pre-commit \"npm run lint-staged\" && npx husky add .husky/pre-commit \"npm run test\" && npx husky add .husky/pre-commit \"npm run e2e:headless\" && echo '#!/bin/sh\n. \"$(dirname \"$0\")/_/husky.sh\"\ncd \"$(dirname \"$0\")/..\" && exec npx --no -- commitlint --edit $1' > .husky/commit-msg && chmod +x .husky/commit-msg && npx husky add .husky/commit-msg \".husky/commit-msg\" && git add .husky/pre-commit .husky/commit-msg",
    "setup:husky-comitlint": "bash husky-config.sh",
    "prepare": "husky install",
    "clean": "rimraf .next .swc out coverage",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@reduxjs/toolkit": "^2.0.1",
    "date-fns": "^3.0.0",
    "date-fns-tz": "^2.0.0",
    "mongoose": "^8.0.3",
    "next": "14.0.4",
    "next-sitemap": "^4.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.12.0",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "swr": "^2.2.4",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "^14.0.4",
    "@storybook/addon-essentials": "^7.6.5",
    "@storybook/addon-interactions": "^7.6.5",
    "@storybook/addon-links": "^7.6.5",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.5",
    "@storybook/nextjs": "^7.6.5",
    "@storybook/react": "^7.6.5",
    "@storybook/test": "^7.6.5",
    "@storybook/test-runner": "^0.16.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18",
    "@types/react-redux": "^7.1.33",
    "@types/redux-logger": "^3.0.12",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.1",
    "daisyui": "^4.4.20",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "next-unused": "^0.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8",
    "redux-logger": "^3.0.6",
    "start-server-and-test": "^2.0.3",
    "storybook": "^7.6.5",
    "tailwindcss": "^3.3.7",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0 <21.0.0"
  }
}
